  "scripts": {
    "start": "react-scripts start",
    "build": "yarn i18n && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "babel": "babel ./src -d ./dist",
    "i18n": "cd ./i18n && node build.js && cd ../",
